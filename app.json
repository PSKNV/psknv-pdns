{
  "name": "PSKNV/psknv-pdns",
  "description": "",
  "keywords": [],
  "repository": "https://github.com/PSKNV/psknv-pdns",
  "stack": "container",
  "env": {
    "PORT": "53",
    "PDNS_dnsupdate": "yes",
    "PDNS_disable-axfr": "yes",
    "PDNS_default-soa-edit-signed": "INCEPTION-EPOCH",
    "PDNS_slave": "no",
    "PDNS_dnssec-key-cache-ttl": "120",
    "PDNS_tcp-fast-open": "1",
    "PDNS_default-ttl": "3600",
    "PDNS_xfr-max-received-mbytes": "0",
    "PDNS_edns-subnet-processing": "yes",
    "PDNS_log-dns-details": "no",
    "PDNS_webserver-port": "8081",
    "PDNS_default-zsk-algorithm": "rsasha256",
    "PDNS_any-to-tcp": "no",
    "PDNS_webserver-password": {
        "generator": "secret"
    },
    "PDNS_expand-alias": "yes",
    "PDNS_disable-syslog": "no",
    "PDNS_version-string": "anonymous",
    "PDNS_gsqlite3-pragma-foreign-keys": "1",
    "PDNS_loglevel": "9",
    "PDNS_gsqlite3-database": "/pdns.sqlite",
    "PDNS_api": "yes",
    "PDNS_default-ksk-size": "4096",
    "PDNS_daemon": "no",
    "PDNS_entropy-source": "/dev/random",
    "PDNS_queue-limit": "15000",
    "PDNS_webserver": "yes",
    "PDNS_receiver-threads": "4",
    "PDNS_local-port": "53",
    "PDNS_direct-dnskey": "yes",
    "PDNS_dname-processing": "no",
    "PDNS_query-local-address": "0.0.0.0",
    "PDNS_launch": "gsqlite3",
    "PDNS_socket-dir": "/",
    "PDNS_include-dir": "/etc/pdns/pdns.d",
    "PDNS_webserver-address": "0.0.0.0",
    "PDNS_load-modules": "gsqlite3",
    "PDNS_query-cache-ttl": "3600",
    "PDNS_gsqlite3-dnssec": "1",
    "PDNS_rng": "openssl",
    "PDNS_setgid": "root",
    "PDNS_default-soa-edit": "INCEPTION-EPOCH",
    "PDNS_negquery-cache-ttl": "3600",
    "PDNS_disable-axfr-rectify": "no",
    "PDNS_config-dir": "/etc/pdns",
    "PDNS_default-zsk-size": "2048",
    "PDNS_max-tcp-connections": "20",
    "PDNS_write-pid": "yes",
    "PDNS_default-ksk-algorithm": "rsasha256",
    "PDNS_log-timestamp": "no",
    "PDNS_log-dns-queries": "no",
    "PDNS_send-signed-notify": "no",
    "PDNS_domain-metadata-cache-ttl": "3600",
    "PDNS_webserver-allow-from": "172.17.0.1,127.0.0.1,::1,10.129.0.22,84.201.139.251,192.168.100.0/24",
    "PDNS_local-address": "0.0.0.0",
    "PDNS_guardian": "no",
    "PDNS_resolver": "1.1.1.1",
    "PDNS_api-key": {
        "generator": "secret"
    },
    "PDNS_reuseport": "yes",
    "PDNS_enable-lua-records": "yes",
    "PDNS_cache-ttl": "3600",
    "PDNS_master": "yes",
    "PDNS_query-logging": "yes",
    "PDNS_udp-truncation-threshold": "1232",
    "PDNS_setuid": "root",
    "PDNS_traceback-handler": "yes",
    "PDNS_non-local-bind": "yes",
    "PDNS_gsqlite3-pragma-synchronous": "0",
    "PDNS_default-api-rectify": "yes"
  }
}
